@page
@model ExamenApp.Pages.IndexModel;
@{
    ViewData["Title"] = "Estudiantes Web App";
}

<h1>Lista de Estudiantes</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Edad</th>
            <th>Correo</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var estudiante in Model.Estudiantes)
        {
            <tr>
                <td>@estudiante.ID</td>
                <td>@estudiante.Nombre</td>
                <td>@estudiante.Apellido</td>
                <td>@estudiante.Edad</td>
                <td>@estudiante.Correo</td>
            </tr>
        }
    </tbody>
</table>